<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewDragLayout">
        <attr name="auto_back" format="boolean" />
        <attr name="horizontal_inside" format="boolean" />
        <attr name="vertical_inside" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EditSelect">
        <attr name="drop_mode" format="enum">
            <enum name="flower_parent" value="0" />
            <enum name="wrap_content" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="GridAutoLayout">
        <attr name="columnNumber" format="integer"></attr>
        <attr name="suggestItemWidth" format="dimension"></attr>
        <attr name="itemWidth" format="dimension"></attr>
        <attr name="paddingVertical" format="dimension"></attr>
        <attr name="paddingHorizontal" format="dimension"></attr>
        <attr name="minItemWidth" format="dimension"></attr>
    </declare-styleable>

</resources>