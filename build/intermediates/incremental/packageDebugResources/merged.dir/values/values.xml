<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView"><attr format="dimension" name="civ_border_width"/><attr format="color" name="civ_border_color"/><attr format="boolean" name="civ_border_overlay"/><attr format="color" name="civ_fill_color"/></declare-styleable>
    <declare-styleable name="CustomImageView"><attr format="dimension" name="civ_borderRadius"/><attr format="reference" name="civ_src"/><attr name="civ_type">
            <enum name="circle" value="0"/>
            <enum name="round" value="1"/>
        </attr></declare-styleable>
    <declare-styleable name="EditSelect"><attr name="drop_mode">
            <enum name="flower_parent" value="0"/>
            <enum name="wrap_content" value="1"/>
        </attr></declare-styleable>
    <declare-styleable name="GridAutoLayout"><attr format="integer" name="columnNumber"/><attr format="dimension" name="suggestItemWidth"/><attr format="dimension" name="itemWidth"/><attr format="dimension" name="paddingVertical"/><attr format="dimension" name="paddingHorizontal"/><attr format="dimension" name="minItemWidth"/></declare-styleable>
    <declare-styleable name="ViewDragLayout"><attr format="boolean" name="auto_back"/><attr format="boolean" name="horizontal_inside"/><attr format="boolean" name="vertical_inside"/></declare-styleable>
    <dimen name="text_size_large">24sp</dimen>
    <dimen name="text_size_medium">20sp</dimen>
    <dimen name="text_size_small">16sp</dimen>
    <dimen name="text_size_small_x">14sp</dimen>
    <dimen name="text_size_small_xx">12sp</dimen>
    <item name="simple_task_tag_key" type="id"/>
    <item name="tag_bitmap_source" type="id"/>
    <item name="tag_view_holder" type="id"/>
    <string name="app_name">AndroidLib</string>
    <string name="empty"/>
    <style name="InputDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowSoftInputMode">stateAlwaysVisible|adjustResize</item>
    </style>
</resources>